{
  "name": "@textile/hub-admin",
  "version": "0.0.0",
  "description": "web-gRPC wrapper client for communicating with Textile's Hub Admin APIs.",
  "main": "dist/index",
  "types": "dist/index",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prepare": "npm run build:node",
    "prebuild": "npm run clean",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "npm run compile",
    "build:browser": "webpack --mode production --config ../../webpack.config.js",
    "compile": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "test:browser": "polendina --cleanup --timeout 60 src/**/*.spec.ts --webpack-config ../../webpack.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-textile.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-textile/issues"
  },
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.12.0",
    "@textile/context": "^0.4.1",
    "@textile/hub-grpc": "^1.0.7",
    "@textile/threads-client": "^0.5.1",
    "loglevel": "^1.6.8"
  },
  "devDependencies": {
    "@textile/threads-core": "^0.1.13",
    "axios": "^0.19.2",
    "delay": "^4.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "references": [
    {
      "path": "../buckets"
    }
  ]
}